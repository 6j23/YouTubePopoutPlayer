{
  "name": "youtube-popout-player",
  "title": "YouTube Popout Player",
  "version": "2.1.0",
  "description": "Provides a simple way to open any YouTube video in a popout window",
  "author": "Ryan Thaut <rthaut@gmail.com> (http://ryanthaut.com)",
  "homepage": "https://github.com/rthaut/YouTubePopoutPlayer",
  "license": "GPL-3.0",
  "scripts": {
    "build": "gulp",
    "build:webextension": "gulp build:webextension",
    "watch": "gulp watch",
    "watch:webextension": "gulp watch:webextension",
    "dev": "npm run build:webextension && npm run start:firefox",
    "start:firefox": "web-ext run --source-dir ./dist/webextension --browser-console"
  },
  "repository": {
    "type": "git",
    "url": "git://https://github.com/rthaut/YouTubePopoutPlayer.git"
  },
  "bugs": {
    "url": "https://github.com/rthaut/YouTubePopoutPlayer/issues"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-browser-i18n-localize": "^1.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-crx-pack": "^1.0.1",
    "gulp-ejs": "^3.0.1",
    "gulp-header": "^1.8.8",
    "gulp-if": "^2.0.2",
    "gulp-uglify": "^3.0.0",
    "gulp-zip": "^4.0.0",
    "run-sequence": "^1.2.2",
    "uglify-es": "^3.0.15",
    "web-ext": "^1.9.1"
  },
  "dependencies": {
    "webextension-polyfill": "^0.1.1"
  }
}
